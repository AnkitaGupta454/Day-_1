#include <iostream>
#include<bits/stdc++.h>
using namespace std;

int main()
{
   int n,k;
    cout<<"Enter length of array and k:";
   cin>>n>>k;
  
   int arr[n];
   cout<<"\n Enter array elements:\n";
   for(int i=0;i<n;i++)
   cin>>arr[i];
   sort(arr,arr+n);
   
cout<<"\n maximun element is:"<<arr[k-1]<<" and minimum element is :"<<arr[n-k];
    return 0;
}
