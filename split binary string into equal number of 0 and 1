#include <iostream>
#include<string.h>
using namespace std;
int main()
{
    string s="0100110101";
    int count0=0,count1=0,cnt=0;
    for(int i=0;i<n;i++)
    {
        if(s[i]=='0')
        count0++;
        else if(s[i]=='1')
        count1++;
        if(count0==count1)
        cnt++;
    }
    if(count0!=count1)
    cout<<"-1";
    else
    cout<<cnt;
    return 0;
}
