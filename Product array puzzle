#include <iostream>
#include<bits/stdc++.h>
using namespace std;

void ProductArray(int a[],int n)
{
    int prod=1;
    for(int i=0;i<n;i++)
    prod=prod*a[i];
    for(int i=0;i<n;i++)
    cout<<prod*pow(a[i],-1)<<" ";
}

int main()
{
    int a[]={10,3,5,6,2};
    int n=sizeof(a)/sizeof(a[0]);
    cout<<"Prodeuct Array is:";
   ProductArray(a,n);
    return 0;
}


outpu:-
Product Array is:180 600 360 300 900 
