#include <iostream>

using namespace std;

int main()
{
   int n;
    cout<<"Enter length of array :";
   cin>>n;
  
   int arr[n];
   cout<<"\n Enter array elements:\n";
   for(int i=0;i<n;i++)
   cin>>arr[i];
   int max=arr[0],min=arr[0];
   for(int i=1;i<n;i++)
   {
       if(arr[i]>max)
       max=arr[i];
       if(arr[i]<min)
       min=arr[i];
   }
cout<<"\n maximun element is:"<<max<<" and minimum element is :"<<min;
    return 0;
}
