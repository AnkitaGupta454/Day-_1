#include <iostream>
#include<bits/stdc++.h>
using namespace std;

int main()
{
    stack<char> s;
   string s1="abcde";
   for(int i=0;i<s1.length();i++)
   s.push(s1[i]);
   string s2="";
   while(!s.empty())
   {
       s2+=s.top();
       s.pop();
   }
   cout<<"\n string after reversing is "<<s2;
    return 0;
}
